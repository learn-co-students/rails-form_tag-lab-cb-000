<h1>Student Form</h1>
<%= form_tag students_path do %>
  <label>First Name:</label>
  <%= text_field_tag "student[first_name]", @student.first_name, id: "first_name" %><br>
  <label>Last Name:</label>
  <%= text_field_tag "student[last_name]", @student.last_name, id: "last_name" %><br>
  <%= submit_tag "Submit Student" %>
<% end %>
