<h3>Student Form</h3>
<%= form_tag students_path do %>
  <label for = "first_name">First name:</label>
  <%= text_field_tag :"student[first_name]",nil ,id: 'first_name' %><br><br>
  <label for= "last_name" >Last name:</label>
  <%= text_field_tag  :"student[last_name]", nil, id: 'last_name'  %>
  <%= hidden_field_tag :"authenticity_token" , form_authenticity_token %><br><br>
  <%= submit_tag "Submit Student", id: "Submit Student"  %>
  <%= params.inspect %>
<% end %>
